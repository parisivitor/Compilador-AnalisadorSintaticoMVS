/*
 * Primeiro programa de teste
 */
programa teste
  inteiro a b // variaveis declaradas
  logico c d nome qualquer
inicio
  leia a   // leitura
  // repeticao
  enquanto a > 0 faca
     b <- 1
     enquanto b < 5 faca
       escreva a
       b <- b + 1
     fimenquanto
     se a > 3 
        entao a <- a - 1
        senao a <- a - 2
     fimse
  fimenquanto 
fimprograma